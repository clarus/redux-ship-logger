{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "Generator": false
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "plugins": ["babel"],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
